<Window x:Class="EquestrianCompetitions.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EquestrianCompetitions"
        mc:Ignorable="d"
        Title="Конные состязания" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="3*"/>

        </Grid.RowDefinitions>
        <Image Source="/Resources/logo.png" HorizontalAlignment="Left"/>
        <TextBlock Text="Конные состязания" FontSize="36" HorizontalAlignment="Center"/>
        <Frame NavigationUIVisibility="Hidden" Grid.Row="1" x:Name="Frame" BorderBrush="Black" BorderThickness="2"/>
    </Grid>
</Window>
